{
  "name": "bandsaws-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist .parcel-cache",
    "prebuild": "npm run clean",
    "build": "BABEL_ENV=parcel parcel build index.html --no-cache",
    "start": "BABEL_ENV=parcel parcel index.html",
    "import-data-to-db": "node --env-file=.env --import tsx data/import-bandsaws-to-db.ts",
    "import-data-to-algolia": "node --env-file=.env --import tsx data/import-bandsaws-to-algolia.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@mui/x-data-grid": "^8.14.1",
    "@radix-ui/react-slider": "^1.3.6",
    "algoliasearch": "^5.41.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-instantsearch": "^7.16.3"
  },
  "devDependencies": {
    "@parcel/core": "^2.16.0",
    "@parcel/packager-raw-url": "^2.16.0",
    "@parcel/transformer-webmanifest": "^2.16.0",
    "@supabase/supabase-js": "^2.75.1",
    "@types/node": "^22.18.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "parcel": "^2.16.0",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
