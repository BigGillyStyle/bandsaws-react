{
  "name": "bandsaws-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist .parcel-cache",
    "prebuild": "npm run clean",
    "build": "BABEL_ENV=parcel parcel build index.html --no-cache",
    "start": "BABEL_ENV=parcel parcel index.html",
    "import-data-to-db": "node --env-file=.env --import tsx data/import-bandsaws-to-db.ts",
    "import-data-to-algolia": "node --env-file=.env --import tsx data/import-bandsaws-to-algolia.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.5",
    "@mui/system": "^7.3.5",
    "@mui/x-data-grid": "^8.24.0",
    "@radix-ui/react-slider": "^1.3.6",
    "algoliasearch": "^5.46.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-instantsearch": "^7.22.1"
  },
  "devDependencies": {
    "@parcel/core": "^2.16.3",
    "@parcel/packager-raw-url": "^2.16.3",
    "@parcel/transformer-webmanifest": "^2.16.3",
    "@supabase/supabase-js": "^2.90.1",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "buffer": "^6.0.3",
    "parcel": "^2.16.3",
    "process": "^0.11.10",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "packageManager": "npm@11.6.2"
}
